{
	"compilerOptions": {
		"paths": {
			"$app/types": [
				"./types/index.d.ts"
			]
		},
		"rootDirs": [
			"../1.3.svelte",
			"./types"
		],
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"lib": [
			"esnext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "bundler",
		"module": "esnext",
		"noEmit": true,
		"target": "esnext"
	},
	"include": [
		"ambient.d.ts",
		"non-ambient.d.ts",
		"./types/**/$types.d.ts",
		"../1.3.svelte/vite.config.js",
		"../1.3.svelte/vite.config.ts",
		"../1.3.svelte/src/**/*.js",
		"../1.3.svelte/src/**/*.ts",
		"../1.3.svelte/src/**/*.svelte",
		"../1.3.svelte/tests/**/*.js",
		"../1.3.svelte/tests/**/*.ts",
		"../1.3.svelte/tests/**/*.svelte"
	],
	"exclude": [
		"../1.3.svelte/node_modules/**",
		"../1.3.svelte/src/service-worker.js",
		"../1.3.svelte/src/service-worker/**/*.js",
		"../1.3.svelte/src/service-worker.ts",
		"../1.3.svelte/src/service-worker/**/*.ts",
		"../1.3.svelte/src/service-worker.d.ts",
		"../1.3.svelte/src/service-worker/**/*.d.ts"
	]
}