{
  "name": "cv-web",
  "version": "1.0.0",
  "description": "Personal CV Website with Sass architecture",
  "scripts": {
    "sass:watch": "sass --watch ../cv_web/3.sass/main.scss:../cv_web/style.css",
    "sass:build": "sass ../cv_web/3.sass/main.scss:../cv_web/style.css --style=compressed",
    "sass:dev": "sass ../cv_web/3.sass/main.scss:../cv_web/style.css --style=expanded"
  },
  "keywords": [
    "cv",
    "portfolio",
    "sass",
    "scss"
  ],
  "author": "Diego Nepomuceno Marcos",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.69.0"
  }
}
